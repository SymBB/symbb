{% trans_default_domain "symbb_frontend" %}

<div class="section_tabs">
    {% block section_tabs %}
    <ul class="nav nav-tabs">
        <li>
            <a href="{{ path('angular_locale_forum_index') }}" title="{{ 'home'|trans }}">
                 <span class="glyphicon glyphicon-home glyphicon-success"></span>
            </a>
        </li>
        <li>
            <a href="{{ path('angular_locale_user_ucp') }}" title="{{ 'profile'|trans }}">
                <span class="glyphicon glyphicon-user"></span>
            </a>
        </li>
        <li>
            <a href="{{ path('angular_locale_user_ucp_security') }}" title="{{ 'security'|trans }}">
                <span class="glyphicon glyphicon-lock"></span>
            </a>
        </li>
        <li>
            <a href="{{ path('angular_locale_message_list') }}" title="{{ 'messages'|trans }}">
                <span class="glyphicon glyphicon-comment"></span>
            </a>
        </li>
    </ul>
    {% endblock %}
</div>

<div class="section_content">
    {% block section_content %}
    {% endblock %}
    <div class="clear"></div>
</div>